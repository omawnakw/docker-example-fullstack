{
  "AWSEBDockerrunVersion": 2,
  "ContainerDefinitions": [
    {
      "name": "client",
      "image": "omawnakw/fullstacklab-client",
      "hostname": "client",
      "essential": false,
      "memory": 128
    },
    {
      "name": "server",
      "image": "omawnakw/fullstacklab-server",
      "hostname": "api",
      "essential": "false",
      "memory": 128
    },
    {
      "name": "worker",
      "image": "omawnakw/fullstacklab-worker",
      "hostname": "worker",
      "essential": "false",
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "omawnakw/fullstacklab-nginx",
      "hostname": "nginx",
      "essential": "true",
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "server"]
    }
  ]
}
